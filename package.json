{
  "name": "faq-generator",
  "version": "1.0.3",
  "description": "AI-powered FAQ generation system from business emails",
  "main": "server.js",
  "scripts": {
    "start": "node --max-old-space-size=2048 --expose-gc server.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm install && npm run build",
    "build:client:dev": "cd client && npm install && node build.js",
    "deploy": "node deploy.js",
    "dev": "nodemon server.js",
    "dev:client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "install:client": "cd client && npm install",
    "test": "mocha tests/**/*.test.js --timeout 10000 --recursive",
    "test:watch": "mocha tests/**/*.test.js --timeout 10000 --recursive --watch",
    "test:coverage": "nyc mocha tests/**/*.test.js --timeout 10000 --recursive",
    "migrate": "node scripts/migrate.js",
    "migrate:simple": "node scripts/migrate-simple.js",
    "seed": "node scripts/seed.js",
    "sync-emails": "node scripts/sync-emails.js",
    "generate-faqs": "node scripts/generate-faqs.js",
    "cleanup": "node scripts/cleanup.js",
    "lint": "eslint src/ tests/ --ext .js",
    "lint:fix": "eslint src/ tests/ --ext .js --fix",
    "postinstall": "npm run install:client"
  },
  "keywords": [
    "faq",
    "ai",
    "nlp",
    "email",
    "automation",
    "customer-support"
  ],
  "author": "FAQ Generator Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "pg": "^8.10.0",
    "redis": "^4.6.5",
    "bull": "^4.10.4",
    "socket.io": "^4.6.1",
    "openai": "^3.2.1",
    "googleapis": "^118.0.0",
    "@azure/msal-node": "^1.17.2",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "multer": "^1.4.5-lts.1",
    "archiver": "^5.3.1",
    "csv-writer": "^1.6.0",
    "csv-parser": "^3.0.0",
    "node-cron": "^3.0.2",
    "joi": "^17.9.1",
    "express-validator": "^6.15.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.0.1",
    "mocha": "^10.2.0",
    "chai": "^4.3.7",
    "sinon": "^15.0.4",
    "supertest": "^6.3.3",
    "nyc": "^15.1.0",
    "eslint": "^8.40.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brinked/faq-generator.git"
  },
  "bugs": {
    "url": "https://github.com/brinked/faq-generator/issues"
  },
  "homepage": "https://github.com/brinked/faq-generator#readme",
  "nyc": {
    "exclude": [
      "tests/**",
      "coverage/**",
      "node_modules/**"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "statements": 80
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": "warn",
      "semi": ["error", "always"]
    }
  }
}
