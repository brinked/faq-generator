{
  "name": "faq-generator",
  "version": "1.0.0",
  "description": "AI-powered FAQ generation from business email accounts",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:client": "cd client && npm install",
    "heroku-postbuild": "npm run install:client && npm run build:client",
    "test": "jest",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "faq",
    "email",
    "ai",
    "nlp",
    "gmail",
    "outlook",
    "customer-service"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "archiver": "^5.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "googleapis": "^128.0.0",
    "@azure/msal-node": "^2.4.0",
    "axios": "^1.6.0",
    "openai": "^4.20.1",
    "node-cron": "^3.0.3",
    "bull": "^4.12.0",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "rate-limiter-flexible": "^3.0.8",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
